{
  "name": "dtdl-visualisation-tool",
  "version": "0.7.16",
  "description": "CLI tool for dtdl visualisation",
  "main": "build/index.js",
  "bin": {
    "dtdl-visualiser": "./build/index.js"
  },
  "type": "module",
  "scripts": {
    "depcheck": "depcheck",
    "test:unit": "NODE_OPTIONS=--no-experimental-strip-types NODE_ENV=test ./node_modules/.bin/mocha --config ./test/mocharc.json ./src/**/*.test.ts",
    "test:e2e": "NODE_ENV=test playwright test --ui",
    "test:playwright": "NODE_ENV=test playwright test --trace on --max-failures=1",
    "test:integration": "NODE_ENV=test ./node_modules/.bin/mocha --config ./test/mocharc.json --file ./test/integration/init.ts ./test/integration/*.test.ts",
    "test:ratelimit": "NODE_OPTIONS=--no-experimental-strip-types NODE_ENV=test ./node_modules/.bin/mocha --config ./test/mocharc.json ./test/ratelimit/*.test.ts",
    "build": "npm run tsoa:build && swc ./src -d ./build --copy-files --strip-leading-paths",
    "clean": "rimraf -rf ./build",
    "dev": "npm run tsoa:build && NODE_ENV=dev node --import '@swc-node/register/esm-register' ./src/index.ts",
    "sample": "npm run tsoa:build && node --import '@swc-node/register/esm-register' ./src/index.ts parse -p ./sample/energygrid | pino-colada",
    "tsoa:build": "tsoa spec-and-routes",
    "local": "npm i -g && dtdl-visualiser",
    "lint": "eslint .",
    "check": "npm run tsoa:build && tsc --noEmit",
    "xss-scan": "xss-scan",
    "db:cmd": "node --import @swc-node/register/esm-register ./node_modules/.bin/knex",
    "db:migrate": "npm run db:cmd -- migrate:latest",
    "db:rollback": "npm run db:cmd -- migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digicatapult/dtdl-visualisation-tool.git"
  },
  "author": "Digital Catapult",
  "license": "Apache-2.0",
  "overrides": {
    "@mermaid-js/mermaid-cli": {
      "puppeteer": "$puppeteer"
    }
  },
  "dependencies": {
    "@digicatapult/dtdl-parser": "^1.1.6",
    "@kitajs/html": "^4.2.11",
    "@kitajs/ts-html-plugin": "^4.1.3",
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/core": "^7.0.6",
    "@tsoa/runtime": "^6.6.0",
    "@types/compression": "^1.8.1",
    "async-mutex": "^0.5.0",
    "body-parser": "^2.2.0",
    "chai": "^6.2.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "envalid": "^8.1.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fuse.js": "^7.1.0",
    "htmx-ext-json-enc": "^2.0.3",
    "htmx-ext-response-targets": "^2.0.4",
    "htmx.org": "^2.0.8",
    "jsdom": "^27.2.0",
    "knex": "^3.1.0",
    "lru-cache": "^11.2.2",
    "mermaid": "^11.12.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.30.0",
    "react": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "svg-pan-zoom": "^3.6.2",
    "tsoa": "^6.6.0",
    "tsyringe": "^4.10.0",
    "unzipper": "^0.12.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@octokit/request-error": "^7.1.0",
    "@playwright/test": "^1.56.1",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.2",
    "@types/chai": "^5.2.3",
    "@types/chai-as-promised": "^8.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "@types/pino-http": "^6.1.0",
    "@types/sinon": "^21.0.0",
    "@types/supertest": "^6.0.3",
    "@types/unzipper": "^0.10.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "chai-as-promised": "^8.0.2",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "otpauth": "^9.4.1",
    "pino-colada": "^2.2.2",
    "playwright-ctrf-json-reporter": "^0.0.26",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.8.1",
    "typescript": "^5.9.3",
    "undici": "^7.16.0"
  },
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  }
}
