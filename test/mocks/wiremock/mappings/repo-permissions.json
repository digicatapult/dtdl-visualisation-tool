{
  "mappings": [
    {
      "request": {
        "method": "GET",
        "urlPathPattern": "/repos/[^/]+/[^/]+"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "jsonBody": {
          "permissions": {
            "pull": true
          }
        }
      }
    }
  ]
}
